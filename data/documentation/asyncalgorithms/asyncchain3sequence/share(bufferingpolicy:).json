fier":"s:Sci7ElementQa"},{"text":", ","kind":"text"},{"text":"Self","kind":"typeIdentifier"},{"text":".","kind":"text"},{"text":"Failure","kind":"typeIdentifier","preciseIdentifier":"s:Sci7FailureQa"},{"text":">\n","kind":"text"}]}]},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The policy controlling how elements are enqueued to the shared buffer. Defaults to ","type":"text"},{"code":".bounded(1)","type":"codeVoice"},{"text":".","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":".bounded(n)"},{"type":"text","text":": Limits the buffer to "},{"type":"codeVoice","code":"n"},{"type":"text","text":" elements, applying backpressure to the source when that limit is reached"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":".bufferingOldest(n)","type":"codeVoice"},{"text":": Keeps the oldest ","type":"text"},{"code":"n","type":"codeVoice"},{"text":" elements, discarding newer ones when full","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".bufferingNewest(n)"},{"text":": Keeps the newest ","type":"text"},{"type":"codeVoice","code":"n"},{"text":" elements, discarding older ones when full","type":"text"}]}]},{"content":[{"inlineContent":[{"code":".unbounded","type":"codeVoice"},{"type":"text","text":": Allows unlimited buffering (use with caution)"}],"type":"paragraph"}]}]}],"name":"bufferingPolicy"}]},{"content":[{"level":2,"text":"Return Value","type":"heading","anchor":"return-value"},{"inlineContent":[{"type":"text","text":"A sendable async sequence that can be safely shared across multiple concurrent tasks."}],"type":"paragraph"}],"kind":"content"},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"code":"share","type":"codeVoice"},{"type":"text","text":" method transforms an async sequence into a shareable sequence that can be safely"},{"type":"text","text":" "},{"type":"text","text":"iterated by multiple concurrent tasks. This is useful when you want to broadcast elements from"},{"type":"text","text":" "},{"type":"text","text":"a single source to multiple consumers without duplicating work or creating separate iterations."}]},{"type":"paragraph","inlineContent":[{"text":"Each element from the source sequence is delivered to all active iterators.","type":"text"},{"text":" ","type":"text"},{"text":"Elements are buffered according to the specified buffering policy to handle timing differences","type":"text"},{"text":" ","type":"text"},{"text":"between consumers.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"The base sequence is iterated in itâ€™s own task to ensure that cancellation is not polluted from","type":"text"},{"text":" ","type":"text"},{"text":"one side of iteration to another.","type":"text"}]},{"text":"Example Usage","type":"heading","level":2,"anchor":"Example-Usage"},{"type":"codeListing","syntax":"swift","code":["let numbers = [1, 2, 3, 4, 5].async.map {"," try? await Task.sleep(for: .seconds(1))"," return $0","}","","let shared = numbers.share()","","\/\/ Multiple tasks can iterate concurrently","let consumer1 = Task {","  for await value in shared {","    print(\"Consumer 1: \\(value)\")","  }","}","","let consumer2 = Task {","  for await value in shared {","    print(\"Consumer 2: \\(value)\")","  }","}","","await consumer1.value","await consumer2.value"]}],"kind":"content"}],"sections":[],"identifier":{"url":"doc:\/\/AsyncAlgorithms\/documentation\/AsyncAlgorithms\/AsyncChain3Sequence\/share(bufferingPolicy:)","interfaceLanguage":"swift"},"references":{"doc://AsyncAlgorithms/documentation/AsyncAlgorithms/AsyncChain3Sequence/share(bufferingPolicy:)":{"type":"topic","url":"\/documentation\/asyncalgorithms\/asyncchain3sequence\/share(bufferingpolicy:)","identifier":"doc:\/\/AsyncAlgorithms\/documentation\/AsyncAlgorit